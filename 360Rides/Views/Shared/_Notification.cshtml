@{
}

@if (TempData["success"] != null)
{
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@Html.Raw("@")pnotify/core@5.2.0/dist/PNotify.min.css"/>
   @*  <link href="node_modules/@Html.Raw("@")pnotify/core/dist/Material.css"
          rel="stylesheet"
          type="text/css" />
    <link rel="stylesheet"
          href="node_modules/material-design-icons/iconfont/material-icons.css" /> *@
    <script src="https://cdn.jsdelivr.net/npm/@Html.Raw("@")pnotify/core@5.2.0/dist/PNotify.min.js"></script>
    <script>
        PNotify.success({
            title: 'Success!',
            text: '@Html.Raw(TempData["Success"])'
        });
    </script>
   
}
@if(TempData["Error"] != null)
{
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@Html.Raw("@")pnotify/core@5.2.0/dist/PNotify.min.css">
    <script src="https://cdn.jsdelivr.net/npm/@Html.Raw("@")pnotify/core@5.2.0/dist/PNotify.min.js"></script>
    <script>
        PNotify.error({
            title: 'Success!',
            text: '@Html.Raw(TempData["Error"])'
        });
    </script>
}